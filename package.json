    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/apollo-upload-client": "17.0.5",
    "@types/jest": "29.5.12",
