    "@mui/x-date-pickers": "6.20.0",
    "@nangohq/frontend": "0.67.8",
    "@sentry/react": "9.46.0",
    "@tanstack/react-virtual": "3.13.12",
    "ace-builds": "1.43.3",
    "actioncable": "5.2.8-1",
    "apollo-link-timeout": "4.0.0",
    "apollo-upload-client": "17.0.0",
    "apollo3-cache-persist": "0.15.0",
    "decimal.js": "10.6.0",
    "formik": "2.4.6",
    "graphql": "16.11.0",
    "graphql-ruby-client": "1.14.8",
    "html-react-parser": "5.2.6",
    "lago-configs": "workspace:*",
    "lago-design-system": "workspace:*",
    "lago-expression": "0.1.6",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "luxon": "3.7.2",
    "prismjs": "1.30.0",
    "react": "18.3.1",
    "react-ace": "11.0.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "react-resizable-panels": "2.1.9",
    "react-router-dom": "6.30.1",
    "recharts": "2.15.4",
    "remark-gfm": "4.0.1",
    "sanitize-html": "2.17.0",
    "shiki": "3.13.0",
    "yup": "1.7.1"
  },
  "engines": {
    "node": ">=22"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "Firefox ESR",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "pnpm": {
    "overrides": {
      "@babel/helpers": "7.28.4",
      "@babel/runtime": "7.28.4"
    }
  }
}
