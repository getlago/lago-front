    "tailwindcss": "^3.4.11",
    "ts-jest": "29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "5.5.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^6.2.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1"
