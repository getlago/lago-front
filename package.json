{
  "name": "lago-front",
  "version": "1.0.0",
  "description": "Front application of Lago",
  "main": "index.js",
  "repository": "git@gitlab.com:getlago/lago-front.git",
  "author": "MorganeLecurieux <morgane.lecurieux.lafayette@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack serve --config ./webpack.dev.js",
    "start:build": "npx http-server-spa ./dist",
    "build": "webpack --config ./webpack.prod.js",
    "build:analyseBundle": "webpack --config ./webpack.prod.js --env analyseBundle",
    "ditto": "npx @dittowords/cli pull",
    "ditto:addNew": "npx @dittowords/cli project add",
    "lint": "prettier --check src/** && eslint \"**/*.{js,ts,tsx}\"",
    "eslint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "prettier --write src/** && eslint --fix .",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "codegen:watch": "graphql-codegen --config codegen.yml -r dotenv/config --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.17.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.6",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@svgr/webpack": "^6.2.1",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/sanitize-html": "^2.6.2",
    "@types/styled-components": "^5.1.22",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "dotenv": "^16.0.0",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@mui/material": "^5.4.2",
    "@mui/styled-engine": "^5.4.2",
    "@mui/styled-engine-sc": "^5.4.2",
    "add": "^2.0.6",
    "apollo-link-timeout": "^4.0.0",
    "apollo-upload-client": "^17.0.0",
    "apollo3-cache-persist": "^0.14.0",
    "classnames": "^2.3.1",
    "crypto-hash": "^2.0.1",
    "formik": "^2.2.9",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "sanitize-html": "^2.7.0",
    "styled-components": "^5.3.3",
    "yarn": "^1.22.17",
    "yup": "^0.32.11"
  },
  "resolutions": {
    "styled-components": "5.3.3",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc",
    "lodash": "4.17.21"
  },
  "engines": {
    "node": ">=14.13"
  },
  "alias": {
    "@mui/styled-engine": "@mui/styled-engine-sc"
  }
}
