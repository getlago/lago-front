{
  "name": "lago-front",
  "version": "1.0.0",
  "description": "Front application of Lago",
  "repository": "git@gitlab.com:getlago/lago-front.git",
  "author": "Lago tech team <tech@getlago.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "packageManager": "pnpm@10.28.0",
  "changelog": {
    "repo": "git@gitlab.com:getlago/lago-front.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build:packages": "pnpm --filter lago-design-system build",
    "postinstall": "pnpm run build:packages",
    "prebuild": "pnpm run build:packages",
    "build": "tsc && vite build",
    "build:win": "tsc & vite build",
    "preview": "vite preview",
    "lint": "prettier --cache --check 'src/**/*.{ts,tsx,svg}' 'index.html' 'cypress/**/*.ts' && eslint --cache --cache-location .eslintcache .",
    "lint:fix": "prettier --write {src,cypress}/** index.html && eslint --fix .",
    "types": "node scripts/check-types.js",
    "code:style": "run-p lint types translations:inspect translations:ensure-consistency",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "codegen:watch": "graphql-codegen --config codegen.yml -r dotenv/config --watch",
    "test": "jest --config jest.config.ts",
    "test:coverage": "jest --config jest.config.ts --coverage --runInBand",
    "test:e2e": "cypress open --config-file cypress/cypress.config.js",
    "translations:add": "node scripts/translations/add",
    "translations:inspect": "node scripts/translations/inspect",
    "translations:ensure-consistency": "node scripts/translations/ensure-translations-consistency",
    "codemod": "node scripts/codemod.js",
    "bundle-analyzer": "pnpx vite-bundle-analyzer",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/preset-env": "7.29.0",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@eslint/compat": "1.4.1",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-operations": "5.0.7",
    "@graphql-codegen/typescript-react-apollo": "4.4.0",
    "@parcel/watcher": "2.5.6",
    "@sentry/vite-plugin": "4.9.0",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@trivago/prettier-plugin-sort-imports": "6.0.2",
    "@types/actioncable": "5.2.11",
    "@types/apollo-upload-client": "17.0.5",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/lodash": "4.17.23",
    "@types/luxon": "3.7.1",
    "@types/node": "24.10.1",
    "@types/prismjs": "1.26.5",
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "@types/react-router-dom": "5.3.3",
    "@types/sanitize-html": "2.16.0",
    "@vitejs/plugin-react-swc": "4.2.2",
    "autoprefixer": "10.4.24",
    "babel-jest": "30.2.0",
    "baseline-browser-mapping": "2.9.19",
    "class-variance-authority": "0.7.1",
    "cssnano": "7.1.2",
    "cypress": "15.10.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.2",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-tailwindcss": "3.18.2",
    "gettext-extractor": "4.0.5",
    "glob": "13.0.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-junit": "16.0.0",
    "jscodeshift": "17.3.0",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "postcss-preset-env": "10.6.1",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwind-merge": "2.6.0",
    "tailwindcss": "3.4.19",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-svgr": "4.5.0",
    "vite-plugin-top-level-await": "1.6.0",
    "vite-plugin-wasm": "3.5.0"
  },
  "dependencies": {
    "@apollo/client": "3.14.0",
    "@ebay/nice-modal-react": "1.2.13",
    "@fragaria/address-formatter": "6.7.1",
    "@llm-ui/code": "0.13.3",
    "@llm-ui/markdown": "0.13.3",
    "@llm-ui/react": "0.13.3",
    "@mui/material": "5.18.0",
    "@mui/x-date-pickers": "6.20.2",
    "@nangohq/frontend": "0.69.29",
    "@sentry/react": "10.38.0",
    "@superset-ui/embedded-sdk": "0.3.0",
    "@tanstack/react-form": "1.28.0",
    "@tanstack/react-virtual": "3.13.18",
    "ace-builds": "1.43.6",
    "actioncable": "5.2.8-1",
    "apollo-link-timeout": "4.0.0",
    "apollo-upload-client": "17.0.0",
    "apollo3-cache-persist": "0.15.0",
    "decimal.js": "10.6.0",
    "formik": "2.4.9",
    "graphql": "16.12.0",
    "graphql-ruby-client": "1.14.9",
    "html-react-parser": "5.2.15",
    "lago-configs": "workspace:*",
    "lago-design-system": "workspace:*",
    "lago-expression": "0.2.0",
    "localforage": "1.10.0",
    "lodash": "4.17.23",
    "lodash-es": "4.17.23",
    "luxon": "3.7.2",
    "prismjs": "1.30.0",
    "react": "18.3.1",
    "react-ace": "14.0.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "react-resizable-panels": "3.0.6",
    "react-router-dom": "6.30.3",
    "recharts": "2.15.4",
    "remark-gfm": "4.0.1",
    "sanitize-html": "2.17.0",
    "shiki": "3.22.0",
    "yup": "1.7.1",
    "zod": "4.3.5"
  },
  "engines": {
    "node": ">=24.11.1"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "Firefox ESR",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "pnpm": {
    "overrides": {
      "@babel/helpers": "7.28.6",
      "@babel/runtime": "7.28.6",
      "js-yaml@^4.0.0": "4.1.1",
      "js-yaml@^3.0.0": "3.14.2",
      "mdast-util-to-hast@^13.0.0": "13.2.1"
    }
  }
}
