{
  "name": "lago-front",
  "version": "1.0.0",
  "description": "Front application of Lago",
  "repository": "git@gitlab.com:getlago/lago-front.git",
  "author": "Lago tech team <tech@getlago.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "packageManager": "pnpm@10.8.0",
  "changelog": {
    "repo": "git@gitlab.com:getlago/lago-front.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build:packages": "pnpm --filter lago-design-system build",
    "postinstall": "pnpm run build:packages",
    "prebuild": "pnpm run build:packages",
    "build": "tsc && vite build",
    "build:win": "tsc & vite build",
    "preview": "vite preview",
    "lint": "prettier --check src/**/*.{ts,tsx,svg} --check index.html --check cypress/**/*.ts && eslint",
    "eslint": "eslint",
    "lint:fix": "prettier --write {src,cypress}/** index.html && eslint --fix .",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "codegen:watch": "graphql-codegen --config codegen.yml -r dotenv/config --watch",
    "test": "jest --config jest.config.ts",
    "test:e2e": "cypress open --config-file cypress/cypress.config.js",
    "translations:add": "node scripts/translations/add",
    "translations:inspect": "node scripts/translations/inspect",
    "changelog:update": "auto-changelog --template=.changelog/regular.hbs",
    "codemod": "node scripts/codemod.js",
    "bundle-analyzer": "vite-bundle-visualizer --sourcemap"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@emotion/react": "11.13.5",
    "@emotion/styled": "11.13.5",
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@parcel/watcher": "^2.3.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "6.4.8",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/apollo-upload-client": "17.0.5",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "4.17.20",
    "@types/luxon": "3.4.2",
    "@types/node": "^22.10.2",
    "@types/prismjs": "1.26.5",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@types/react-router-dom": "5.3.3",
    "@types/sanitize-html": "2.9.5",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "auto-changelog": "2.4.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "class-variance-authority": "^0.7.0",
    "cssnano": "^7.0.6",
    "cypress": "15.2.0",
    "dotenv": "16.3.1",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "gettext-extractor": "3.8.0",
    "glob": "^11.0.0",
    "globals": "^15.12.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jscodeshift": "^17.1.1",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.13",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^6.3.6",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.4.1"
  },
  "dependencies": {
    "@apollo/client": "3.11.4",
    "@llm-ui/code": "^0.13.3",
    "@llm-ui/markdown": "^0.13.3",
    "@llm-ui/react": "^0.13.3",
    "@mui/material": "5.14.18",
    "@mui/x-date-pickers": "6.20.0",
    "@nangohq/frontend": "^0.67.8",
    "@sentry/react": "9.46.0",
    "@tanstack/react-virtual": "3.11.2",
    "ace-builds": "^1.37.3",
    "apollo-link-timeout": "4.0.0",
    "apollo-upload-client": "17.0.0",
    "apollo3-cache-persist": "0.15.0",
    "decimal.js": "^10.4.3",
    "formik": "2.4.6",
    "graphql": "^16.6.0",
    "html-react-parser": "^5.2.6",
    "lago-configs": "workspace:*",
    "lago-design-system": "workspace:*",
    "lago-expression": "0.1.6",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "lodash-es": "^4.17.21",
    "luxon": "3.4.4",
    "prismjs": "1.30.0",
    "react": "18.2.0",
    "react-ace": "^11.0.1",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.9",
    "react-router-dom": "6.15.0",
    "recharts": "^2.15.1",
    "remark-gfm": "^4.0.1",
    "sanitize-html": "2.12.1",
    "shiki": "^3.12.2",
    "yup": "1.2.0"
  },
  "engines": {
    "node": ">=20"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "Firefox ESR",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "pnpm": {
    "overrides": {
      "@babel/helpers": "7.26.10",
      "@babel/runtime": "7.26.10"
    }
  }
}
